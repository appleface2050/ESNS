<form ajax callback="customer_service_create_sys_column_callback" class="form-horizontal" role="form">


    <div class="form-group" input-group>
        <label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 名称 </label>

        <div class="col-sm-9">
            <input name="name" input-check="required" id="form-field-2" placeholder="名称"
                   class="col-xs-10 col-sm-5">
        </div>
    </div>
    <div class="form-group" input-group>
        <label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 顺序 </label>
        <div class="col-sm-9">
            <input name="index_num" id="" placeholder="顺序"
                   class="col-xs-10 col-sm-5">
        </div>
    </div>


    <div class="clearfix form-actions">
        <div class="col-md-offset-3 col-md-9">
            <button class="btn btn-info" type="submit">
                <i class="icon-ok bigger-110"></i>
                提交
            </button>
        </div>
    </div>
</form>