<form ajax callback="customer_service_create_company_callback" class="form-horizontal" role="form">

    <div class="form-group" input-group>
        <label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 公司名称 </label>
        <div class="col-sm-9">
            <input name="name" input-check="required" id="" placeholder="公司名称"
                   class="col-xs-10 col-sm-5">
        </div>
    </div>
    <div class="form-group" input-group>
        <label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 地址 </label>
        <div class="col-sm-9">
            <input name="address" input-check="required" id="" placeholder="地址"
                   class="col-xs-10 col-sm-5">
        </div>
    </div>
    <div class="form-group" input-group>
        <label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 联系电话 </label>
        <div class="col-sm-9">
            <input name="phone" input-check="required" id="" placeholder="联系电话"
                   class="col-xs-10 col-sm-5">
        </div>
    </div>
    <div class="form-group" input-group>
        <label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 集团 </label>

        <div class="col-sm-9">
            <select name="bigcompany_id" input-check="required" id="form-field-bigcompany_id" class="col-sm-5" placeholder="集团">
            <%for(var i=0;i<bigcompany_list.length;i++){%>
                <option value="<%=bigcompany_list[i].id%>" <%if(bigcompany_list[i].id==bigcompany_id){%>selected<%}%>><%=bigcompany_list[i].name%></option>
            <%}%>
            </select>
        </div>
    </div>
    <div class="clearfix form-actions">
        <div class="col-md-offset-3 col-md-9">
            <button class="btn btn-info" type="submit">
                <i class="icon-ok bigger-110"></i>
                提交
            </button>
        </div>
    </div>
</form>


