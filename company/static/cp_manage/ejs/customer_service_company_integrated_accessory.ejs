<form ajax callback="customer_service_create_company_integrated_accessory_callback" class="form-horizontal" role="form">

   <div class="form-group" input-group>
        <label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 标题 </label>

        <div class="col-sm-9">
            <input name="pre_title" id="form-field-2" placeholder="标题"
                   class="col-xs-10 col-sm-5" input-check="required">
        </div>
    </div>

    <div class="form-group" input-group>
        <label class="col-sm-3 control-label no-padding-right" for="form-field-2"> 附件 </label>
        <div class="col-sm-9">
            <button class="btn btn-default" id="btn_select_file" type="button">选择附件</button>
            <div id="upload_list"></div>
            <input type="hidden" name="fileid" id="logoimage">
        </div>
    </div>
    <div class="clearfix form-actions">
        <div class="col-md-offset-3 col-md-9">
            <button class="btn btn-info" type="submit">
                <i class="icon-ok bigger-110"></i>
                提交
            </button>
            <button class="btn btn-default" type="button" onClick="goNewsBack()">
                返回
            </button>
             <input type="hidden" name="id" id="news_id" value="<%=id%>">
        </div>
    </div>
</form>